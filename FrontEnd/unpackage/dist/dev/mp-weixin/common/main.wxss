view,
scroll-view,
swiper,
swiper-item,
cover-view,
cover-image,
icon,
text,
rich-text,
progress,
button,
checkbox,
form,
input,
label,
radio,
slider,
switch,
textarea,
navigator,
audio,
camera,
image,
video {
  box-sizing: border-box;
  font-family: "PingFang SC", "Microsoft YaHei", Arial, Helvetica, sans-serif;
}
view {
  font-size: 24rpx;
}
input {
  border: none;
}
.clamp {
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.clamp-two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.clear-fl::after {
  content: "";
  display: block;
  clear: both;
}
image {
  display: block;
  width: 100%;
  height: auto;
}
/* button样式改写 */
uni-button,
button {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: normal;
}
uni-button.no-border:before, uni-button.no-border:after, button.no-border:before, button.no-border:after {
  border: 0;
}
uni-button[type="default"],
button[type="default"] {
  color: #494949;
}
