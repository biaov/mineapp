/* 字体图标居中 */
.iconfont {
  vertical-align: middle;
}
/* 状态栏 */
.u-status-bar {
  width: 100%;
  height: 25px;
}
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.4s;
}
.fade-enter,
.fade-leave-to {
  opacity: 0;
}
.slide-enter-active,
.slide-leave-active {
  transition: all 0.5s linear;
}
.slide-enter {
  -webkit-transform: translateY(100%) scale(1);
          transform: translateY(100%) scale(1);
  opacity: 1;
}
.slide-leave-to {
  -webkit-transform: translateY(-100%) scale(1);
          transform: translateY(-100%) scale(1);
  opacity: 0;
}
.m-mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  opacity: 0;
  transition: all 0.4s;
}
.m-mask.show {
  opacity: 1;
}
/* 横向滚动 */
@-webkit-keyframes rollAnimation {
0% {
    -webkit-transform: translateX(100%);
            transform: translateX(100%);
}
50% {
    -webkit-transform: translateX(0%);
            transform: translateX(0%);
}
100% {
    -webkit-transform: translateX(-100%);
            transform: translateX(-100%);
}
}
@keyframes rollAnimation {
0% {
    -webkit-transform: translateX(100%);
            transform: translateX(100%);
}
50% {
    -webkit-transform: translateX(0%);
            transform: translateX(0%);
}
100% {
    -webkit-transform: translateX(-100%);
            transform: translateX(-100%);
}
}
/* 图标旋转 */
@-webkit-keyframes iconRotate {
0% {
    -webkit-transform: translate(-50%, -50%) rotate(0deg);
            transform: translate(-50%, -50%) rotate(0deg);
}
50% {
    -webkit-transform: translate(-50%, -50%) rotate(180deg);
            transform: translate(-50%, -50%) rotate(180deg);
}
100% {
    -webkit-transform: translate(-50%, -50%) rotate(360deg);
            transform: translate(-50%, -50%) rotate(360deg);
}
}
@keyframes iconRotate {
0% {
    -webkit-transform: translate(-50%, -50%) rotate(0deg);
            transform: translate(-50%, -50%) rotate(0deg);
}
50% {
    -webkit-transform: translate(-50%, -50%) rotate(180deg);
            transform: translate(-50%, -50%) rotate(180deg);
}
100% {
    -webkit-transform: translate(-50%, -50%) rotate(360deg);
            transform: translate(-50%, -50%) rotate(360deg);
}
}
@-webkit-keyframes iconRotate2 {
0% {
    -webkit-transform: translate(-50%, -50%) rotate(0deg);
            transform: translate(-50%, -50%) rotate(0deg);
}
50% {
    -webkit-transform: translate(-50%, -50%) rotate(-180deg);
            transform: translate(-50%, -50%) rotate(-180deg);
}
100% {
    -webkit-transform: translate(-50%, -50%) rotate(-360deg);
            transform: translate(-50%, -50%) rotate(-360deg);
}
}
@keyframes iconRotate2 {
0% {
    -webkit-transform: translate(-50%, -50%) rotate(0deg);
            transform: translate(-50%, -50%) rotate(0deg);
}
50% {
    -webkit-transform: translate(-50%, -50%) rotate(-180deg);
            transform: translate(-50%, -50%) rotate(-180deg);
}
100% {
    -webkit-transform: translate(-50%, -50%) rotate(-360deg);
            transform: translate(-50%, -50%) rotate(-360deg);
}
}
/* 旋转 */
.rotate {
  display: block;
  -webkit-animation: iconRotate 1.6s linear infinite;
          animation: iconRotate 1.6s linear infinite;
}
.rotate2 {
  display: block;
  -webkit-animation: iconRotate2 1.6s linear infinite;
          animation: iconRotate2 1.6s linear infinite;
}
